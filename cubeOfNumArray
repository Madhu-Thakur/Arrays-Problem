
public class cubeOfNumArray {
    public static void main(String[] args) {
        int arr[]={2,3,2,5,6,4};
       
         for(int i=0; i<arr.length; i++){
             
            System.out.println( Math.sqrt(arr[i]));
        }
        for(int i=0; i<arr.length; i++){
             
            System.out.println(Math.pow(arr[i],3));
        }
}
}
