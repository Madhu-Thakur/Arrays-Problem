public class sumOfOddNUm {
    public static void main(String[] args) {
        
   
        int arr[] = { 23, 4, 6, 4, 5, 7, 8, 43, 2 };
        double sum=0 ;
       
        for( int i = 0;i<arr.length;i++)
        {
            if (arr[i] % 2 != 0) {
                sum = sum + arr[i];
            }
        }
        System.out.println("odd:"+sum);
    }
}
