public class countTheUpCaseChar {
    public static void main(String[] args) {
        char a[] = {'N','O','2','6',' ','S','o','u','n','d'};
        int count = 0;
        
        for(int i=0;i<a.length; i++){
            if(Character.isDigit(a[i]))
            count++;
            }
            System.out.println(count);
    }
}
