public class stringArrays {
    public static void main(String[] args) {
        String[] fruits = {"Apple", "Banana", "Cherry", "Date", "Elderberry"};

        for (int i = 0; i < fruits.length; i++) {
            if (fruits[i].length() >= 4) {
                System.out.println(fruits[i].substring(0, 4));
            }
        }
    }
}
