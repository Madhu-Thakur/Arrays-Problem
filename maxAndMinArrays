public class maxAndMinArrays {
    public static void main(String[] args) {
        int a[] = {5,12,4,21,8,2,3,7,9,11};
        int max = a[0];
        int min = a[0];
        for(int i=0;i<a.length;i++){
            if(a[i]>max)
             max = a[i];
             if(a[i]<min)
                min = a[i];
        }
        System.out.println(max);
        System.out.println(min);
    }
}

